@page
@model TestWorkLanit.Pages.Doctors.CreateModel
@{
    ViewData["Title"] = "Добавление нового врача";
}
<h2>@ViewData["Title"]</h2>
<div asp-validation-summary="All"></div>
<form method="POST">
    <div><label>Имя</label><br /><input asp-for="Doctor.Name" /></div>
    <div><label>Специализация</label><br /><input asp-for="Doctor.Spec" /></div>
    <div><label>Работает с </label><br /><input asp-for="Doctor.TimeFrom" type="number" /></div>
    <div><label>Работает до</label><br /><input asp-for="Doctor.TimeTo" type="number" /></div>

    <input type="submit" value="Отправить" />
</form>
